FUNCTION "osr.edw.staging.rms::TF_FORM_0IDP_001_CURRENT_VER" ( )
       RETURNS TABLE (
    	CLIENT		NVARCHAR(3),
        OBJECT_ID	NVARCHAR(32),
        VERSION 	NVARCHAR(5)
       )
       LANGUAGE SQLSCRIPT 
       SQL SECURITY INVOKER AS 
BEGIN 

RETURN
SELECT 
CLIENT,
OBJECT_ID,
MAX(VERSION) AS VERSION
FROM 
"osr.edw.staging.td.rms::CDS_TRM.DSO.Z1000IDP00000000.active_data"
GROUP BY CLIENT, OBJECT_ID
;

END;